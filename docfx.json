{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "./**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "./**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": ["apispec/*.md"],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [
      null
    ],
    "dest": "."
  }
}
